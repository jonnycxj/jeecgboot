<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.jgds.order.mapper.ERPJgdsOrderCustomMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  jgds_order_custom 
		WHERE
			 order_main_id = #{mainId} 
	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.jgds.order.entity.ERPJgdsOrderCustom">
		SELECT * 
		FROM  jgds_order_custom
		WHERE
			 order_main_id = #{mainId} 
	</select>
</mapper>
